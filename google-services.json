{
  "project_info": {
    "project_number": "${EXPO_PUBLIC_PROJECT_NUMBER}",
    "firebase_url": "${EXPO_PUBLIC_FIREBASE_URL}",
    "project_id": "${EXPO_PUBLIC_FIREBASE_PROJECT_ID}",
    "storage_bucket": "${EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET}"
  },
  "client": [
    {
      "client_info": {
        "mobilesdk_app_id": "${EXPO_PUBLIC_MOBILESDK_APP_ID}",
        "android_client_info": {
          "package_name": "${EXPO_PUBLIC_PACKAGE_NAME}"
        }
      },
      "oauth_client": [
        {
          "client_id": "${EXPO_PUBLIC_ANDROID_CLIENT_ID}",
          "client_type": 3
        }
      ],
      "api_key": [
        {
          "current_key": "${EXPO_PUBLIC_API_KEY}"
        }
      ],
      "services": {
        "appinvite_service": {
          "other_platform_oauth_client": [
            {
              "client_id": "${EXPO_PUBLIC_OAUTH_CLIENT_ID_ANDROID}",
              "client_type": 3
            },
            {
              "client_id": "${EXPO_PUBLIC_OAUTH_CLIENT_ID_IOS}",
              "client_type": 2,
              "ios_info": {
                "bundle_id": "${EXPO_PUBLIC_BUNDLE_ID}"
              }
            }
          ]
        }
      }
    }
  ],
  "configuration_version": "1"
}
