        "EXPO_PUBLIC_FIREBASE_APP_ID": "$EXPO_PUBLIC_FIREBASE_APP_ID",
        "EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID": "$EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID",
        "EXPO_PUBLIC_GOOGLE_CLIENT_ID": "$EXPO_PUBLIC_GOOGLE_CLIENT_ID",
        "EXPO_PUBLIC_GOOGLE_CLIENT_SECRET": "$EXPO_PUBLIC_GOOGLE_CLIENT_SECRET"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "$EXPO_PUBLIC_API_URL",
        "EXPO_PUBLIC_GOOGLE_MAPS_API": "$EXPO_PUBLIC_GOOGLE_MAPS_API",
